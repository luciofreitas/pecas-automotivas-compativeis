{
  "name": "pecas-automotivas-compativeis",
  "version": "0.0.1",
  "private": true,
  "homepage": "https://luciofreitas.github.io",
  "description": "Web UI (React + Vite) para catálogo de peças automotivas; use `npm run dev:chrome` para abrir o site automaticamente no Google Chrome sem alterar o navegador padrão do sistema.",
  "scripts": {
    "dev": "vite",
    "dev:chrome": "concurrently \"npm run dev\" \"wait-on http://localhost:5174 && .\\open-chrome.bat\"",
    "build": "vite build",
    "preview": "vite preview",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "test-db": "node scripts/test-db.js",
    "seed": "node scripts/seed.js"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/material": "^7.3.1",
    "daisyui": "^5.0.50",
    "firebase": "^12.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.8.1",
    "react-select": "^5.10.2"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.12",
    "@vitejs/plugin-react": "^4.0.0",
    "autoprefixer": "^10.4.21",
    "concurrently": "^9.2.1",
    "dotenv": "^16.6.1",
    "gh-pages": "^6.3.0",
    "open-cli": "^8.0.0",
    "pg": "^8.16.3",
    "postcss": "^8.5.6",
    "puppeteer": "^24.20.0",
    "tailwindcss": "^4.1.12",
    "vite": "^7.1.2",
    "wait-on": "^8.0.4"
  }
}
