# For Vite, prefix env vars with VITE_ (these are preferred). For Create React App use REACT_APP_* names.
VITE_FIREBASE_API_KEY=your_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id

# CRA / fallback names (optional)
REACT_APP_FIREBASE_API_KEY=your_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your_project_id
# Optional:
REACT_APP_FIREBASE_APP_ID=your_app_id
# Database configuration (copy to .env and fill with real values)
DATABASE_URL=postgresql://username:password@localhost:5432/dbname

# Optional: Backend port
PORT=3001
